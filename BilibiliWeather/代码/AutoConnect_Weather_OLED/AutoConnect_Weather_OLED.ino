#include <ESP8266WiFi.h>
#include <ESP8266_Seniverse.h>
#include <ESP8266HTTPClient.h>
         
#include <DNSServer.h>
#include <ESP8266WebServer.h>
#include <WiFiManager.h> 

#include <ArduinoJson.h>
#include <U8g2lib.h>

#ifdef U8X8_HAVE_HW_SPI
#include <SPI.h>
#endif
#ifdef U8X8_HAVE_HW_I2C
#include <Wire.h>
#endif

U8G2_SSD1306_128X64_NONAME_F_HW_I2C u8g2(U8G2_MIRROR, /* reset=*/ U8X8_PIN_NONE);

void setup() {
  Serial.begin(115200);                    // 初始化串口设置
  Wire.begin(1, 3);
  u8g2.begin();
  u8g2.enableUTF8Print();                  // enable UTF8
  u8g2.clearBuffer();                      // 清空显存
  
  u8g2.setFont(u8g2_font_wqy12_t_gb2312a); // 选一个合适的字体
  u8g2.setCursor(40, 40);
  u8g2.print("开始联网...");   
  u8g2.sendBuffer();                       // 打到屏幕上
 
  WiFiManager wifiManager;                 // 建立WiFiManager对象
  wifiManager.autoConnect("yizhipao");     // 自动连接WiFi（连接ESP8266时的WiFi名称）
  
  u8g2.clearDisplay();
  u8g2.setCursor(40, 40);
  u8g2.print("联网成功");   
  u8g2.sendBuffer();
  delay(1000);
  u8g2.clearDisplay();
  
}
 
void loop() {
  u8g2.clearDisplay();
  
//  DisplayTime();  //显示时间
  DisplayTianqi();//显示天气

  u8g2.sendBuffer();
  
  delay(60000);   //每1min更新一次
}

