  if (!client.connect("api.k780.com", 80)) {
    Serial.println(F("Connection failed"));
    return;
  }
  Serial.println(F("Connected!"));
  
  // 建立请求资源地址
  String reqRes = "/?app=life.time&appkey=10003&sign=b59bc3ef6191eb9f747dd4e83c99f2a4&format=json";

  // 建立http请求信息
  String httpRequest = String("GET ") + reqRes + " HTTP/1.1\r\n" + 
                              "Host: api.k780.com\r\n" + 
                              "Connection: close\r\n\r\n";
                              
  // 向服务器发送http请求信息
  client.print(httpRequest);

  // 获取并显示服务器响应状态行
  String status_response = client.readStringUntil('\n');
  Serial.print("status_response: ");
  Serial.println(status_response);

  // 使用find跳过HTTP响应头
  if (client.find("\r\n\r\n")) {
    Serial.println("Found Header End. Start Parsing.");
    return;
  }
  // 利用ArduinoJson库解析响应信息  
  const size_t capacity = JSON_OBJECT_SIZE(2) + JSON_OBJECT_SIZE(7) + 170;
  DynamicJsonDocument doc(capacity);
  
//  const char* json = "{\"success\":\"1\",\"result\":{\"timestamp\":\"1597627921\",\"datetime_1\":\"2020-08-17 09:32:01\",\"datetime_2\":\"2020年08月17日 09时32分01秒\",\"week_1\":\"1\",\"week_2\":\"星期一\",\"week_3\":\"周一\",\"week_4\":\"Monday\"}}";
  
