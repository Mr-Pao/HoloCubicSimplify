//    return;
//  }
  
  const char* success = doc["success"]; // "1"
  
  JsonObject result = doc["result"];
  const char* result_timestamp = result["timestamp"]; // "1597627921"
  const char* result_datetime_1 = result["datetime_1"]; // "2020-08-17 09:32:01"
  const char* result_datetime_2 = result["datetime_2"]; // "2020年08月17日 09时32分01秒"
  const char* result_week_1 = result["week_1"]; // "1"
  const char* result_week_2 = result["week_2"]; // "星期一"
  const char* result_week_3 = result["week_3"]; // "周一"
  const char* result_week_4 = result["week_4"]; // "Monday"

  String jsonCode;  
  serializeJson(doc, jsonCode);
  Serial.print("Root Json Code: ");Serial.println(jsonCode); 
  
